<h1>Listing benmarks</h1>
<%= link_to 'New Benmark', new_benmark_path %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Url</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @benmarks.each do |benmark| %>
      <tr>
        <td><%= link_to benmark.url, benmark.url %></td>
        <td><%= link_to 'Edit', edit_benmark_path(benmark) %>
        <%= link_to 'Destroy', benmark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>